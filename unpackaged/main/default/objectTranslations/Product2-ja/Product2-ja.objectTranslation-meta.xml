    <validationRules>
        <errorMessage><!-- Product Description cannot exceed 500 caracters --></errorMessage>
        <name>DescriptionLengthControl</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The end of commercialization date can&apos;t be inferior to the current date + 31 days (maximum period of a quote validity) --></errorMessage>
        <name>End_of_Commercialization_Date_Check</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The Internal Description is required. --></errorMessage>
        <name>Required_ShortDescription_Tier_Product</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You cannot check &quot;No Royalty&quot; and another type of Royalty at the same time. --></errorMessage>
        <name>Royalty_No_Royalty</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You have to set a Royalties Type for this Article. --></errorMessage>
        <name>Royalty_Type_Mandatory</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Je dois obligatoirement, lorsque je créé un produit avec une structure d&apos;activité de niveau 1 SaaS, y associer un ou plusieurs niveaux de service parmi la liste des niveaux existants. --></errorMessage>
        <name>SaaS_Prdt_must_have_at_least_1_serv_Lvl</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Non-modifiable data following Synchro with Zuora. --></errorMessage>
        <name>Synchro_Date_Zuora</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- If you want to insert a Product with a Charge Type equals to &apos;Usage&apos;, you have to fill the Consumption Code. --></errorMessage>
        <name>VR003_UsageRequiresConsumptionCode</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You must enter at least one possible Billing Mode for each Product and each Package. --></errorMessage>
        <name>VR005_BillingModesMustBeFilled</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You can&apos;t register a Charge with Excess Pricing Method equals to Main Charge Unit Price if the Main Charge Product has a null Excess Unit Price Coeff. --></errorMessage>
        <name>VR006_MainChargeUnitPriceIssue</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You are saving an Article with Quantity Declarative as Royalties Type. Buying Metric, Purchasing Price, Purchased Quantity and Declarative Royalties are mandatory. --></errorMessage>
        <name>VR007_Buying_Metric_Mandatory</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Please, fill the Charge Type field for a non-Article Product. --></errorMessage>
        <name>VR016_ChargeTypeAlwaysRequired</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You can&apos;t synchronize a product if it does not have a Structure Level 1, 2 and 3, a Product Line, a Core Business and a Billing Type. Please, review the Product. --></errorMessage>
        <name>VR018_Cant_Sync_if_missing_info</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Allowed Quantities Field is not properly filled. Please, write only &quot;ALL&quot;, or a list of allowed quantities separated by semicolons. Don&apos;t forget a semicolon at the end. --></errorMessage>
        <name>VR_013_Allowed_Quantities_Field_Format</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- If Pricing Method is List, please fill a Metric. If no Metric can be applied, please choose Flat Fee value by default. --></errorMessage>
        <name>VR_018_Metric_when_List</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- For a Schedulable Article, Duration is mandatory. Please fill this field --></errorMessage>
        <name>VR_Prod_SchedulableDurationMandatory</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- For a Schedulable Article, Service Term is mandatory and can&apos;t be &quot;Flat Fee&quot;. --></errorMessage>
        <name>VR_Prod_SchedulableServiceLevelMandatory</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You can&apos;t set the Product Status to Inactive if you are not a CPQ Expert or a System Administrator. --></errorMessage>
        <name>VR_Validate_Inactive_Status_Transition</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You can only change the Tier Status to Closed if you are a CPQ Specialist, and if the end of commercialization date is already passed --></errorMessage>
        <name>Validate_Closed_Tier_Status_Transition</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Supplier is required --></errorMessage>
        <name>Validation_of_checkbox_royalties</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- At least one of the royalties must be selected --></errorMessage>
        <name>Validation_of_checkbox_royalties_F</name>
    </validationRules>
