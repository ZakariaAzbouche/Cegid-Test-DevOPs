<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR006_Account_Partner</fullName>
    <active>true</active>
    <errorConditionFormula>AND (
				ISBLANK(TEXT(Partnership__c)),
				ISPICKVAL(Type__c,"003")
				)</errorConditionFormula>
    <errorDisplayField>Partnership__c</errorDisplayField>
    <errorMessage>The partnership field is mandatory for partners.</errorMessage>
</ValidationRule>
