<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR01_EndDateBeforeNextRenewalDate</fullName>
    <active>false</active>
    <errorConditionFormula>CancellationDate__c &lt; NextDateRenewal__c</errorConditionFormula>
    <errorDisplayField>CancellationDate__c</errorDisplayField>
    <errorMessage>La date de fin ne peut pas être antérieure à la date de renouvellement.</errorMessage>
</ValidationRule>
